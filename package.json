  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.17.0",
    "@types/express-socket.io-session": "^1.3.2",
    "@types/node": "^13.7.7",
    "@types/socket.io": "^2.1.4",
    "cross-env": "^7.0.0",
