{
  "name": "tg-websocket-2",
  "version": "1.0.0",
  "description": "Tg Websocket for Webclient 2.0",
  "main": "server.js",
  "scripts": {
    "prod": "concurrently --kill-others \"npm run start:api\"  \"npm run start:server\"",
    "dev": " cross-env NODE_ENV=development concurrently --kill-others \"npm run start:api\"  \"npm run start:server\"",
    "start:server": "ts-node-dev --respawn --clear --debounce=1000 --inspect=0.0.0.0:4445 server.ts",
    "start:api": "ts-node-dev --respawn --clear --debounce=1000  --inspect=0.0.0.0:4446 api.ts",
    "prod:server": "tsc --build && node build/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karedas/tg-websocket-2.git"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "ws",
    "websocket",
    "thegate"
  ],
  "author": "Andrea Lisi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/karedas/tg-websocket-2/issues"
  },
  "homepage": "https://github.com/karedas/tg-websocket-2#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.9.5",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/express-socket.io-session": "^1.3.2",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "@types/socket.io-client": "^1.4.32",
    "babel-jest": "^25.4.0",
    "cross-env": "^7.0.2",
    "jest": "^25.4.0",
    "ts-jest": "^25.4.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/minimist": "^1.2.0",
    "@types/socket.io": "^2.1.4",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "concurrently": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-socket.io-session": "^1.3.5",
    "minimist": "^1.2.5",
    "socket.io": "^2.3.0",
    "winston": "^3.2.1"
  }
}
